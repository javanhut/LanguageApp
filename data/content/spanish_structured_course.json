        { "id": "l10_7", "type": "input", "prompt": "Is there WiFi?", "answer": ["¿hay wifi?", "hay wifi", "¿hay internet?"], "hints": ["Modern necessity"] },
        { "id": "l10_8", "type": "input", "prompt": "Could you repeat that?", "answer": ["¿puedes repetir?", "puedes repetir", "¿puede repetir?", "repite por favor"], "hints": ["Clarification"] },
