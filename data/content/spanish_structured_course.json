        { "id": "l10_4", "type": "input", "prompt": "I'm lost", "answer": ["estoy perdido", "estoy perdida", "me perdí"], "hints": ["Need directions"] },
        { "id": "l10_5", "type": "input", "prompt": "How do I get to...?", "answer": ["¿cómo llego a?", "como llego a", "¿cómo voy a?"], "hints": ["Asking directions"] },
